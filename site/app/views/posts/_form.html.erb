
<%= form_with model: @post do |form|%>
    <div>
        <%= form.label :name %>
        <div class="">
            <%= form.text_field :name, class: "bg-white text-black border border-gray-300 p-2 rounded w-[400px]" %>
        </div>
    </div>

    <div>
        <%= form.submit%>
    </div>
<% end %>